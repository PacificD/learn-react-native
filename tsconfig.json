{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@navigation": [
        "src/navigation"
      ],
      "@components": [
        "src/components"
      ],
      "@screens": [
        "src/screens"
      ],
      "@assets": [
        "assets"
      ]
    }
  }
}